{
  "name": "line-density",
  "version": "0.0.2",
  "description": "Fast density heatmaps for massive time series.",
  "main": "build/line-density.js",
  "module": "build/index.js",
  "unpkg": "build/line-density.min.js",
  "jsdelivr": "build/line-density.min.js",
  "typings": "build/index.d.ts",
  "author": "Dominik Moritz",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@types/ndarray": "^1.0.6",
    "gh-pages": "^1.2.0",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.9.4",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "typescript": "^2.9.2",
    "vega-embed": "^3.16.0"
  },
  "scripts": {
    "start": "parcel demo/index.html",
    "prebuild": "rm -rf build",
    "build:tsc": "tsc src/index.ts --outDir build --allowSyntheticDefaultImports --esModuleInterop -t es6 -d",
    "build:rollup": "rollup -c",
    "build": "npm run build:tsc && npm run build:rollup",
    "postbuild": "uglifyjs build/line-density.js -c -m -o build/line-density.min.js",
    "predeploy": "rm -rf dist && parcel build --public-url '.' demo/index.html",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "ndarray": "^1.0.18",
    "regl": "^1.3.7"
  }
}
